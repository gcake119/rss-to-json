{
  "name": "rss-to-json-all",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && cp -r data dist/",
    "deploy": "npm run build && rm -rf docs && mkdir docs && cp -r dist/* docs/ && git add docs && git commit -m 'deploy: update docs' && git push" ,
    "preview": "vite preview",
    "build:podcast": "node scripts/rss_to_json_podcast.js",
    "build:newsletter": "node scripts/rss_to_json_newsletter.js"
  },
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "date-fns": "^3.6.0",
    "fast-xml-parser": "^4.4.1",
    "feedparser": "^2.2.10",
    "marked": "^16.4.0",
    "node-fetch": "^3.3.2",
    "sanitize-html": "^2.13.0"
  },
  "devDependencies": {
    "vite": "^7.1.9"
  }
}
